<script
  type="module"
  src="https://unpkg.com/@splinetool/viewer@1.9.0/build/spline-viewer.js"
></script>
<spline-viewer
  id="mySplineViewer"
  loading-anim-type="spinner-small-dark"
  url="https://prod.spline.design/q5yCrwh5mbGSoTau/scene.splinecode"
></spline-viewer>

<script>
  document.addEventListener("DOMContentLoaded", () => {
    const splineViewer = document.getElementById("mySplineViewer");
    

    if (splineViewer) {
      // Wait for the Spline Viewer to load
      splineViewer.addEventListener("load-start", (e) => {
        const spline = splineViewer?.shadowRoot?.getElementById("spline");
        const aTagSpline = splineViewer?.shadowRoot?.getElementById("logo");
        if(spline) spline.style.borderRadius = '40px'
        if(aTagSpline) aTagSpline.style.opacity = '0'
      });
    }
  });
</script>
